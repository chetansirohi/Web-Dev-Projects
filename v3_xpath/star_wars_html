<script>

    function CTOinit() {

      if (window.CTO) {
        cto = new CTO();
        cto.account = "wdgdollucas,wdgdolstarcom,wdgdsec,matterhorn";
        cto.categoryCode = "luc";
        cto.siteCode = "starwars";
        cto.breadcrumbs="news:characters-and-histories";
        cto.pageName="15-star-wars-quotes-to-use-in-everyday-life";
        cto.pageId="53917";
        cto.postDate="2015-05-12 15:00:50";
        cto.brandSegment = "luc";
        cto.templateType = "news";
        cto.propertyCode = "sws";
        cto.buCode = "luc";
        cto.articleid = "53917";        cto.track();//Required. Do not remove.

        //Detect Quiz container.
        var swQuizObj = document.getElementsByClassName( 'tm-quiz-container' );

        //Only trigger article_start if article does not contain a quiz/poll
        if( !swQuizObj.length ) {
          cto.trackEvent( { "engagementType":"article_start" } );        }

        var isPollType = $( '.tm-quiz-container' ).hasClass( 'quiz-poll' );

        //If quiz container is present, trigger trackEvent.
        if( swQuizObj.length > 0 ) {
          if ( isPollType ) {
            cto.trackEvent( {'engagementType':'poll_start'} );
          } else {
            cto.trackEvent( {'engagementType':'quiz_start'} );
          }
        }
      }
    }

</script>

